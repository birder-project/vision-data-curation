[flake8]
# E203: whitespace before ':' - handled by black
# E704: multiple statements on one line - handled by black
# W503: line break before binary operator - handled by black
ignore = E203,E704,W503

max-line-length = 120
exclude =
    .git,
    __pycache__,
    .venv,
    data,

pep585-activation = always
